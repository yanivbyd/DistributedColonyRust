    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Backend",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/backend",
            "args": [],
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "RUST_LOG",
                    "value": "debug"
                }
            ],
            "preLaunchTask": "cargo-build-backend",
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
